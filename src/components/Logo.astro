---
export interface Props {
	width?: string;
    height?: string;
}

let { width, height } = Astro.props;

width = width ?? '200px';
height = height ?? '200px';
---
<img src="/logo.png" />
<style define:vars={{ width, height }}>
    img {
        width: var(--width);
        height: var(--height);
        object-fit: contain;
    }
</style>
